<Window x:Class="Llave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Llave"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="800">
    <Canvas>
        <Path Stroke="Black"
              Fill="Black"
              Data="M400,100 
              H700 
              L740,150 
              L700,180 
              L660,150 
              L620,180 
              L580,150 
              L540,180 
              L500,150 
              L460,200 
              L400,200
              M400,100
              A100,100 0 1 0 400,200"></Path>
        <Ellipse Width="35"
                 Height="35"
                 Fill="White"
                 Canvas.Left="250"
                 Canvas.Top="100"></Ellipse>
    </Canvas>
</Window>
