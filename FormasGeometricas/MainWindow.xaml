                    <CombinedGeometry.Geometry1>
                        <RectangleGeometry Rect="0,0,50,50"></RectangleGeometry>
                    </CombinedGeometry.Geometry1>
                    <CombinedGeometry.Geometry2>
                        <EllipseGeometry Center="50,25" RadiusX="30" RadiusY="10"></EllipseGeometry>
                    </CombinedGeometry.Geometry2>
                </CombinedGeometry>
            </Path.Data>
        </Path>-->
    </Canvas>
</Window>
